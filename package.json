{
	"name": "postcss-media-query-importer",
	"version": "1.1.0",
	"description": "PostCSS plugin to import media queries",
	"author": "thomasefbland",
	"keywords": [
		"postcss",
		"css",
		"postcss-plugin",
		"postcss-media-query-importer",
		"media queries",
		"media query"
	],
	"scripts": {
		"test": "jest",
		"build": "rm -r -f dist && rollup -c rollup.config.mjs"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"default": "./dist/index.mjs"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"engines": {
		"node": ">=12.0.0"
	},
	"peerDependencies": {
		"postcss": "^8.4.21"
	},
	"devDependencies": {
		"@release-it/bumper": "^4.0.2",
		"@rollup/plugin-typescript": "^11.0.0",
		"jest": "^29.3.1",
		"postcss": "^8.4.21",
		"release-it": "^15.6.0",
		"rollup": "^3.13.0",
		"rollup-plugin-multi-input": "^1.3.3",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5"
	},
	"jest": {
		"modulePaths": [
			"<rootDir>/test"
		]
	}
}
